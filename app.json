{
	"name": "Heroku aria2c",
	"description": "Run aria2c on heroku",
	"repository": "https://github.com/maple3142/heroku-aria2c",
	"keywords": ["aria2c"],
	"env": {
		"HEROKU_APP_NAME": {
			"description": "This should be the same as \"App name\" above.",
			"value": "xinxin8816"
		},
		"ARIA2C_SECRET": {
			"description": "Aria2c's secret",
			"value": "xinxin8816"
		},
		"RCLONE_CONFIG": {
			"description": "Rclone config to mount in one line, replace line breaks with \"\\n\"",
			"required": false
		},
		"RCLONE_CONFIG2": {
			"description": "Rclone config2 to mount in one line, replace line breaks with \"\\n\"",
			"required": false
		},
		"RCLONE_DESTINATION": {
			"description": "",
			"required": false,
			"value": "tmp"
		},
		"RCLONE_DESTINATION2": {
			"description": "",
			"required": false,
			"value": "LoveAction"
		}
	},
	"buildpacks": [
		{
			"url": "heroku/nodejs"
		}
	]
}
